
import java.util.Scanner;

public class KeepInputUntilUserInputsx {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int x=sc.nextInt();
        int sum=0;
        int count=0;
        while(count!=x)
        {
            sum=sum+n;
            count++;
        }
        System.out.println(sum);
    }
    
}
